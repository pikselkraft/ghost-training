{{!< default}}
{{> header}}
<style>
    iframe {
        height: 31.25rem;
    }

</style>
<main>
<div class="fx-row fx-row-center-l">
    <div class="fx-col-80-l">
    {{#post}}
    <article class="pcontact {{post_class}}" role="article" itemscope itemtype="http://schema.org/Article">
        <header class="pcontact--header">
            <h1 class="pcontact--title" itemprop="headline">{{title}}</h1>
